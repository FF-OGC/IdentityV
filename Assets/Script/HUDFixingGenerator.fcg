import "Hud.fcc" as Hud
import "StdLibrary.fcc" as StdLib
import "EditorGenLib.fcc" as EditorLib
import Resources from "EditorGenLib.fcc"
import "./PlayerFixingHUD.fcg" as PlayerFixingHUD

graph HUDFixingGenerator {
    event HUD_ButtonClicked(button entity<HudButtonWidget>, player entity<Player>) {
        if button == player<PlayerFixingHUD>.GetFixingHUDWidget(EResource_UI_FIXING.BTN_SKILLCHECK) {
            player<PlayerFixingHUD>.SelectSkillCheck()
        }
    }
    
}